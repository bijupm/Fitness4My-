<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.data.fitness4me"
    android:installLocation="auto"
    android:versionCode="5"
    android:versionName="0.0" >

    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
     <uses-permission android:name="android.permission.KILL_BACKGROUND_PROCESSES" />
    <supports-screens
        android:anyDensity="true"
        android:largeScreens="true"
        android:normalScreens="true"
        android:smallScreens="true" />

    <permission
        android:name="com.data.fitness4me.permission.C2D_MESSAGE"
        android:protectionLevel="signature" />

    <uses-permission android:name="com.data.fitness4me.permission.C2D_MESSAGE" />
    <!-- This app has permission to register and receive message -->
    <uses-permission android:name="com.google.android.c2dm.permission.RECEIVE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.STATUS_BAR" >
    </uses-permission>

    <uses-sdk android:minSdkVersion="9" android:targetSdkVersion="13" />

    <application
        android:name="com.data.pack.FitnessApplication"
        android:icon="@drawable/app_icon"
        android:killAfterRestore="true"
        android:label="@string/app_name"
        android:largeHeap="true" >
        <service android:name="com.data.pack.MyC2dmReceiver"  />
        <service
            android:name="com.data.pack.ServiceTemplate"
            android:enabled="true" />

        <receiver
            android:name="com.data.pack.MyC2dmReceiver" 
            android:permission="com.google.android.c2dm.permission.SEND" >

            <!-- Receive the actual message -->
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE" />
                <action android:name="android.intent.action.SCREEN_ON" />

                <category android:name="com.data.fitness4me" />
            </intent-filter>
            <!-- Receive the registration id -->
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.REGISTRATION" />

                <category android:name="com.data.fitness4me" />
            </intent-filter>
        </receiver>

        <activity
            android:name="com.data.pack.upgradepopup"
            android:screenOrientation="landscape"
            android:theme="@style/Theme.D1NoTitleDim"
            android:windowSoftInputMode="adjustPan" >
        </activity>
        <activity
            android:name="com.data.pack.fulldownload"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.D1NoTitleDim" >
        </activity>
        <activity
            android:name="com.data.pack.freepurchasedownload"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.D1NoTitleDim" >
        </activity>
        <activity
            android:name="com.data.pack.congrats"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.D1NoTitleDim" >
        </activity>
        <activity
            android:name="com.data.pack.upgradeborder"
            android:screenOrientation="landscape"
            android:theme="@android:style/Theme.NoTitleBar.Fullscreen" >
        </activity>
        <activity
            android:name="com.data.pack.videostart"
            android:screenOrientation="landscape"
            android:theme="@android:style/Theme.NoTitleBar.Fullscreen" >
        </activity>
        <activity
            android:name="com.data.pack.videoplay"
            android:screenOrientation="landscape"
            android:theme="@android:style/Theme.NoTitleBar.Fullscreen" >
        </activity>
        <activity
            android:name="com.data.pack.popup"
            android:screenOrientation="landscape"
            android:windowSoftInputMode="adjustPan" >
        </activity>
        <activity
            android:name="com.data.pack.spinner"
            android:screenOrientation="landscape"
            android:theme="@android:style/Theme.NoTitleBar.Fullscreen" >
        </activity>
        <activity
            android:name="com.data.pack.evalution"
            android:screenOrientation="portrait"
            android:theme="@android:style/Theme.NoTitleBar.Fullscreen" >
        </activity>
        <activity
            android:name="com.data.pack.Feedback"
            android:screenOrientation="portrait"
            android:theme="@android:style/Theme.NoTitleBar.Fullscreen" >
        </activity>
        <activity
            android:name="com.data.pack.Download"
            android:screenOrientation="portrait"
            android:theme="@android:style/Theme.NoTitleBar.Fullscreen" >
        </activity>
        <activity
            android:name="com.data.pack.Feedbackfinish"
            android:screenOrientation="portrait"
            android:theme="@android:style/Theme.NoTitleBar.Fullscreen" >
        </activity>
        <activity
            android:name="com.data.pack.welcomepage"
            android:screenOrientation="portrait"
            android:theme="@android:style/Theme.NoTitleBar.Fullscreen" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.data.pack.FitnessforMeActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@android:style/Theme.NoTitleBar.Fullscreen" >
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

